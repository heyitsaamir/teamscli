    "@inquirer/prompts": "^7.2.1",
    "adm-zip": "^0.5.16",
    "commander": "^12.1.0",
    "env-paths": "^4.0.0",
    "nanospinner": "^1.2.2",
    "picocolors": "^1.1.1"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.7",
    "@types/node": "^22.10.0",
    "tsx": "^4.19.0",
